# This file is managed by the 'files-matatika-lab' file bundle and updated automatically when `meltano upgrade` is run.
# To prevent any manual changes from being overwritten, remove the file bundle from `meltano.yml` or disable automatic updates:
#     meltano config --plugin-type=files files-matatika-lab set _update orchestrate/matatika/elastic-search/config/es_datasets_index_config.json false

{
  "settings": {
    "number_of_shards": 2,
    "number_of_replicas": 1
  },
  "mappings": {
    "dynamic": "true",
    "_source": {
      "enabled": "true"
    },
    "properties": {
      "id": {
		"type": "text",
			"fields": {
				"keyword": {
					"type": "keyword",
					"ignore_above": 256
				}
			}
	  },
      "title": {
        "type": "text"
      },
      "description": {
        "type": "text"
      },
      "lastModified": {
        "type": "date",
        "format": "strict_date_optional_time_nanos"
      },
      "lastModifiedMillisDay": {
        "type": "integer"
      },
      "text_vector": {
        "type": "dense_vector",
        "dims": 768
      }
    }
  }
}
